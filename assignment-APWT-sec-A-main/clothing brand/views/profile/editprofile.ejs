<h1 style="green"> Profile</h1>


		<fieldset>
			<legend>Profile </legend>
			<form  method="post" >
			<table>
				<tr>
					<td>username</td>
					<td><input type="text" name="username" value="<%= username %>"></td>
				</tr>
				<tr>
					<td>email</td>
					<td><input type="email" name="email" value="<%= email %>"></td>
				</tr>
				<tr>
					<td>phone no</td>
					<td><input type="text" name="phone_no" value="<%= phoneno %>"></td>
				</tr>
				<tr>
					<td>gender</td>
					<td><input type="text" name="gender" value="<%= gender %>"></td>
				</tr>
				<tr>
					<td>password</td>
					<td><input type="password" name="password" value="<%= password %>"></td>
				</tr>
				<tr>
					<td>type</td>
					<td><input type="number" name="type" value="<%= type %>" readonly="true"></td>
				</tr>
				

				<tr>
					<td></td>
					<td><input type="submit" name="submit" value="Submit"></td>
				</tr>
			</table>
		</fieldset>
	</form>

	
	


